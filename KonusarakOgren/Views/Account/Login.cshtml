
@{
    ViewData["Title"] = "Login";
}

<div class="login-container">
    <h3>KULLANICI GİRİŞ</h3>
    <form method="post">
    <input class="form-control" type="text" placeholder="Kullanıcı Adı" id="UserName" name="UserName"/>
    <input class="form-control" type="password" placeholder="Şifre" id="Password" name="Password"/>
    <div class="login-button">
        <button type="submit"  class="btn btn-primary" id="login">Giriş Yap</button>
    </div>
    </form>
</div>

